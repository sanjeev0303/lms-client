{
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm run build:production",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/page.tsx": {
      "maxDuration": 30
    }
  },
  "crons": [],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_test_ZnJhbmstbGFicmFkb3ItOTMuY2xlcmsuYWNjb3VudHMuZGV2JA",
    "CLERK_SECRET_KEY": "sk_test_xxfGhU9fLfZi48EdY1eO8ET9Dsr5iinnu5rSKtNmrv",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL": "/sign-in",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL": "/sign-up",
    "NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL": "/",
    "NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL": "/",
    "NEXT_PUBLIC_API_URL": "/api",
    "NEXT_PUBLIC_RAZORPAY_KEY_ID": "rzp_test_R6piEg43ylFzxw",
    "ANALYZE": "false",
    "NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING": "true",
    "NEXT_PUBLIC_ENABLE_PREFETCHING": "true",
    "NEXT_PUBLIC_API_TIMEOUT": "3000",
    "NEXT_PUBLIC_RETRY_ATTEMPTS": "3",
    "NEXT_PUBLIC_CACHE_TIME": "300000",
    "NEXT_PUBLIC_ENABLE_DEVTOOLS": "false",
    "NEXT_PUBLIC_ENABLE_OFFLINE_MODE": "false"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://lms-server-lw51.onrender.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
